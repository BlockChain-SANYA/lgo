package lgo_exec

import pkg0 "github.com/yunabe/lgo/core"

func fn() {
}

type MyType struct {
	Name string
	Age  int
}

func (m *MyType) Hello() string {
	return "Hello " + m.Name
}

type MyInterface interface {
	DoSomething(x int) float32
	Hello() string
}

func lgo_init() {
	pkg0.LgoRegisterVar("x", &x)
	pkg0.LgoRegisterVar("y", &y)
	pkg0.LgoRegisterVar("alice", &alice)
	pkg0.LgoRegisterVar("bob", &bob)
	pkg0.LgoRegisterVar("i", &i)
	pkg0.LgoRegisterVar("j", &j)
	x = 10
	y = "hello"
	alice = &MyType{"Alice", 12}
	bob = &MyType{"Bob", 45}
	i = alice
	j = bob
}

const (
	c = "constant"
	d = 123
)

var (
	x     int
	y     string
	alice *MyType
	bob   *MyType
	i     interface{}
	j     interface{}
)
