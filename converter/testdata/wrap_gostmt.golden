package lgo_exec

import pkg0 "github.com/yunabe/lgo/core"

func lgo_init() {
	pkg0.LgoRegisterVar("f", &f)
	f = func(x, y int) int {
		return x + y
	}
	go func() {
		defer pkg0.FinalizeGoRoutine()
		func(x int) {
			go func() {
				defer pkg0.FinalizeGoRoutine()
				f(x, 20)
			}()
		}(10)
	}()
}

var (
	f func(x int, y int) int
)
